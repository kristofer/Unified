// Test Set with Int type
// Tests basic set operations with integers

fn testSetInt() -> Bool {
    let set = Set<Int>.new()
    
    // Test empty
    if !set.isEmpty() {
        return false
    }
    
    if set.len() != 0 {
        return false
    }
    
    // Test insert
    let added = set.insert(10)
    if !added {
        return false
    }
    
    set.insert(20)
    set.insert(30)
    
    if set.len() != 3 {
        return false
    }
    
    // Test duplicate insert
    added = set.insert(10)
    if added {
        return false
    }
    
    if set.len() != 3 {
        return false
    }
    
    // Test contains
    if !set.contains(10) {
        return false
    }
    
    if !set.contains(20) {
        return false
    }
    
    if set.contains(99) {
        return false
    }
    
    // Test remove
    let removed = set.remove(20)
    if !removed {
        return false
    }
    
    if set.len() != 2 {
        return false
    }
    
    if set.contains(20) {
        return false
    }
    
    // Test remove non-existent
    removed = set.remove(99)
    if removed {
        return false
    }
    
    // Test clear
    set.clear()
    if !set.isEmpty() {
        return false
    }
    
    return true
}

fn main() -> Int {
    if testSetInt() {
        return 1  // Success
    }
    return 0  // Failure
}

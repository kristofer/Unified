// Test Stack with Float type
// Tests basic stack operations with floating point numbers

fn testStackFloat() -> Bool {
    let stack = Stack<Float>.new()
    
    // Test empty
    if !stack.isEmpty() {
        return false
    }
    
    // Test push
    stack.push(3.14)
    stack.push(2.71)
    stack.push(1.41)
    
    if stack.len() != 3 {
        return false
    }
    
    // Test peek
    if stack.peek() != 1.41 {
        return false
    }
    
    // Test pop (LIFO order)
    let val = stack.pop()
    if val != 1.41 {
        return false
    }
    
    val = stack.pop()
    if val != 2.71 {
        return false
    }
    
    if stack.len() != 1 {
        return false
    }
    
    // Test peek
    if stack.peek() != 3.14 {
        return false
    }
    
    // Test contains
    if !stack.contains(3.14) {
        return false
    }
    
    // Test clear
    stack.clear()
    if !stack.isEmpty() {
        return false
    }
    
    return true
}

fn main() -> Int {
    if testStackFloat() {
        return 1  // Success
    }
    return 0  // Failure
}

// Compound assignment examples
fn compound_ops() -> Int {
    let mut total = 100
    total += 50      // 150
    total -= 30      // 120
    total *= 2       // 240
    total /= 4       // 60
    total %= 25      // 10
    return total
}

fn main() -> Int {
    return compound_ops()  // Returns 10
}

// Test 9: Generic comparison function with trait bounds
// Tests constrained generics

interface Comparable {
    fn compare(self, other: Self) -> Int
}

fn max<T: Comparable>(a: T, b: T) -> T {
    if a.compare(b) > 0 {
        return a
    }
    return b
}

fn min<T: Comparable>(a: T, b: T) -> T {
    if a.compare(b) < 0 {
        return a
    }
    return b
}


// Test 13: Generic function with conditional logic
// Tests that generic code can contain control flow

fn max<T>(a: T, b: T) -> T {
    if a > b {
        return a
    }
    return b
}

fn main() -> Int {
    let result = max(10, 20)
    
    if result == 20 {
        return 1
    }
    return 0
}

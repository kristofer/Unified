// Test 6: Generic swap function
// Tests in-place operations with generic types

fn swap<T>(a: &mut T, b: &mut T) {
    let temp = *a
    *a = *b
    *b = temp
}

fn main() -> Int {
    let mut x = 10
    let mut y = 20
    
    swap(&mut x, &mut y)
    
    if x == 20 && y == 10 {
        return 1
    }
    return 0
}

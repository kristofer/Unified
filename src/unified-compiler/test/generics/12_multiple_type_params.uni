// Test 12: Multiple generic parameters with different types
// Tests a function that takes two different generic type parameters

fn first<A, B>(a: A, b: B) -> A {
    return a
}

fn second<A, B>(a: A, b: B) -> B {
    return b
}

fn main() -> Int {
    let x = first(10, "hello")
    let y = second(10, 20)
    
    if x == 10 && y == 20 {
        return 1
    }
    return 0
}

// Test 11: Generic function with explicit type arguments
// Tests calling a generic function with explicit type parameters

fn wrap<T>(value: T) -> T {
    return value
}

fn main() -> Int {
    // Call with explicit type argument
    let x = wrap<Int>(10)
    let y = wrap<Int>(20)
    
    if x + y == 30 {
        return 1
    }
    return 0
}

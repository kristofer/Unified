// Test 5: Generic pair function with multiple type parameters
// Tests functions with 2+ type parameters

fn pair<A, B>(a: A, b: B) -> (A, B) {
    return (a, b)
}

fn first<A, B>(tuple: (A, B)) -> A {
    return tuple.0
}

fn second<A, B>(tuple: (A, B)) -> B {
    return tuple.1
}

fn main() -> Int {
    let p = pair(42, "hello")
    let f = first(p)
    let s = second(p)
    
    if f == 42 {
        return 1
    }
    return 0
}
